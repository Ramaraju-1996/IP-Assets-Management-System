 <p id="notice"><%= notice %></p>
<div class="jumbotron abc">
<h1>Ip Assets</h1>

<table>
  <thead>
    <tr>
      <th>Research group id</th>
      <th>Type</th>
      <th>Labs associated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ip_assets.each do |ip_asset| %>
      <tr>
        <td><%= ip_asset.Research_group_ID %></td>
        <td><%= ip_asset.Type %></td>
        <td><%= ip_asset.Labs_Associated %></td>
        <td><%= link_to 'Show', ip_asset %></td>
        <td><%= link_to 'Edit',:controller=>"ip_assets",:action=>"edit_asset",:ip_asset=>ip_asset %></td>
        <td><%= link_to 'Destroy',:controller=>"ip_assets",:action=>"delete_asset",:ip_asset=>ip_asset %></td> 
        <!-- ip_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
    <% if current_user.try(:admin?) or current_user.try(:ip_committee?) %>
    <td><%= link_to "Approve ",:controller=>"ip_assets",:action=>"set_approved",:ip_asset=>ip_asset%></td>
    <td><%= link_to "Approve Edit ",:controller=>"ip_assets",:action=>"set_edit",:ip_asset=>ip_asset%></td>
    <td><%= link_to "Reject Edit ",:controller=>"ip_assets",:action=>"set_not_edit",:ip_asset=>ip_asset%></td>
    <td><%= link_to "Give royalty",:controller=>"ip_assets",:action=>"share_royalty",:ip_asset=>ip_asset%></td>
   <% end %>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ip Asset', new_ip_asset_path %>
</div>
