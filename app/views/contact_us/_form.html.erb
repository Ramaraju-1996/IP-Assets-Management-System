<%= simple_form_for(@contact_u) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :contact_number %>
    <%= f.input :contact_email_id %>
    <%= f.input :Tell_us_more_about_you %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
